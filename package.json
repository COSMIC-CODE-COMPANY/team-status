  "main": "index.js",
  "scripts": {
    "clean": "rm -r dist",
    "mkdir": "mkdir -p dist/assets && mkdir -p dist/translations",
    "copy:assets": "cp -r src/assets/* dist/assets",
    "build:manifest": "node src/util/build.js",
    "build:dev": "npm run clean && npm run mkdir && npm run copy:assets && npm run build:manifest && npx webpack -d",
    "build:dev:zd": "npm run copy:assets && npm run build:manifest && npx webpack -d",
    "build:prod": "npm run clean && npm run mkdir && npm run copy:assets && npm run build:manifest && npx webpack -p",
    "watch": "npm run clean && npm run mkdir && npm run copy:assets && npm run build:manifest && npx webpack -d --watch",
    "start": "webpack-dev-server"
  },
